{
  "expo": {
    "name": "AI Product Lister",
    "slug": "ai-product-lister",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "ai-product-lister",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667EEA"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiproductlister.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#667EEA"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.aiproductlister.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "30ffd233-53dc-41a1-89ff-2f9cfab97de5"
      }
    },
    "owner": "saifi.dev04"
  }
}
